                            <input type="text" class="form-control form-control-sm" placeholder="NCM aqui" name="ncm" id="ncm" onkeypress="return onlynumber();" size=10 maxlength=10 />

                            <div class="input-group-append">
                                <!--botao-->
                                <button id="salvarNCMTribNCM" type="submit" class="btn btn-secondary pr-botoes-fundo" data-toggle="tooltip" data-placement="right" title="Salvar NCM em Massa">

                                    <i style="font-size:1.3em;" class="fas fa-save"></i>
                                </button>
                            </div>
                        </div>
                    </div>

