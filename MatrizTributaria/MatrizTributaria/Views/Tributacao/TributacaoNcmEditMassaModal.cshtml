                            <select id="selAlterar" class="pr-fontalternativa form-control form-control-sm" style="max-width:100% !important;">

                                <option value="1">Não Alterar</option>
                                <option value="2">Alterar</option>
                                <option value="3">Anular Cest</option>


                            </select>

                            <div class="input-group-append">
                                <!--botao-->
                                <button id="mesmoNCM" type="submit" class="btn btn-secondary pr-botoes-fundo" data-toggle="tooltip" data-placement="right" title="Salvar NCM em Massa">

                                    <i style="font-size:1.3em;" class="fas fa-check-circle"></i>
                                </button>
                            </div>
                        </div>
