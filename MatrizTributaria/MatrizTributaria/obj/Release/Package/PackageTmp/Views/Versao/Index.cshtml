@model IEnumerable<MatrizTributaria.Models.Versao>

@{
    ViewBag.Title = "Versao";
}

<div class="container-fluid pr-container pr-card-cad">
    <div class="card mt-2">
        <div class="card-header">
            Cadastro de Versões - LISTAGEM
        </div><!--fim do cabeçalho-->
        <div class="card-body">
            <div class="container">
                <div class="row">
                    @RenderPage("~/Views/Acoes/Botoes.cshtml") <!--Renderiza os botões laterais-->
                    <div class="col-12">
                        <div class="container-fluid">
                            <table id="tablepr" class="table table table-light table-hover thead-dark table-striped">
                                <thead class="pr-thead">
                                    <tr>
                                        <th>@Html.DisplayNameFor(model => model.id)</th>
                                        <th>@Html.DisplayNameFor(model => model.nota) </th>
                                        <th>@Html.DisplayNameFor(model => model.versao)</th>
                                        
                                    </tr>
                                </thead>
                                <tbody>

                                    @foreach (var item in Model)
                                    {
                        <tr>
                            <td>@Html.DisplayFor(modelItem => item.id)</td>
                            <td>@Html.DisplayFor(modelItem => item.nota)</td>
                            <td>@Html.DisplayFor(modelItem => item.versao)</td>
                            
                        </tr>}
                                </tbody>
                            </table>
                        </div><!--Fim do container da coluna-->
                    </div><!--fim da coluna de 10-->
                </div><!--fim da linha-->
            </div><!--fim do container do body do card-->
        </div><!--Fim do body do card-->

    </div> <!--Fom do card-->


</div>