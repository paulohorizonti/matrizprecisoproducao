<a href="@Url.Action("HomeCliente", "HomeCliente")" data-toggle="tooltip" data-placement="right" title="Voltar para página Home" class="float-lg-right dis-block btn btn-secondary pr-botoes-fundo border-0"><i style="font-size:1.5em;" class="fas fa-home"></i></a><!--inicio-->
<button type="button" id="visualizarDados" data-toggle="tooltip" data-placement="right" title="Exibir detalhes do registro selecionado" class="float-lg-right dis-block btn btn-secondary pr-botoes-fundo border-0"> <i style="font-size:1.5em;" class="fas fa-info-circle"></i></button><!--Detalhes-->

@if (Session["nivel"].Equals("USUARIO"))
{


    <button style="pointer-events: none; opacity: 0.6;" type="button" id="apagarDados" class="float-lg-right dis-block btn btn-secondary pr-botoes-fundo border-0" data-toggle="tooltip" data-placement="top" title="Excluir item selecionado"> <i style="font-size:1.5em;" class="fas fa-trash-alt"></i></button><!--Apagar-->
    <button style="pointer-events: none; opacity: 0.6;" data-toggle="tooltip" data-placement="right" title="Alterar registro selecionado" type="button" id="editarDados" class="float-lg-right dis-block btn btn-secondary pr-botoes-fundo border-0"> <i style="font-size:1.5em;" class="fas fa-edit"></i></button><!--Editar-->
    <a style="pointer-events: none; opacity: 0.6;" data-toggle="tooltip" data-placement="right" title="Adicionar Novo Registro" class="float-lg-right dis-block btn btn-secondary pr-botoes-fundo border-0 " href="@Url.Action("CreateUsuarioCliente", "TributacaoEmpresa")"><i style="font-size:1.5em;" class="fas fa-plus-circle"></i></a><!--Novo-->
}
else
{
    <button type="button" id="apagarDados" data-toggle="tooltip" data-placement="right" title="Excluir registro selecionado" class="float-lg-right dis-block btn btn-secondary pr-botoes-fundo border-0"> <i style="font-size:1.5em;" class="fas fa-trash-alt"></i></button><!--Apagar-->
    <button type="button" id="editarDados" data-toggle="tooltip" data-placement="right" title="Alterar registro selecionado" class="float-lg-right dis-block btn btn-secondary pr-botoes-fundo border-0"> <i style="font-size:1.5em;" class="fas fa-edit"></i></button><!--Editar-->
    <a class="float-lg-right dis-blockbtn btn btn-secondary pr-botoes-fundo border-0" data-toggle="tooltip" data-placement="right" title="Adicionar Novo Registro" href="@Url.Action("CreateUsuarioCliente", "TributacaoEmpresa")"><i style="font-size:1.5em;" class="fas fa-plus-circle"></i></a><!--Novo-->



}

