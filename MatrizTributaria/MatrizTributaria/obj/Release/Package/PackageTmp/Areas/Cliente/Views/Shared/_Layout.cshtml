        <script>
            //mascaras
            $(document).ready(function () {

                $("#cest").mask("99.999.99");
                $(".cest").mask("99.999.99");
                $("#ncm").mask("9999.99.99");
                $(".decimal").mask("9999,999");
                $('#cep').mask('99.999-999');
                $('#telefone').mask('(99)99999-9999');
                $(".senha").mask("xxxxxxxxx");
            });


            $(document).ready(function () {


                $("#parFiltro").change(function () {


                    var escolha = document.getElementById("parFiltro");
                    var value = escolha.options[escolha.selectedIndex].value;
                    if (value == "3") {
                        $('#FiltroDados').val("");
                        document.getElementById("FiltroDados").setAttribute('placeholder', "Digite descrição ou o código de Barras");

                        $('#FiltroDados').unmask();
                    }
                    if (value == "2") {
                        $('#FiltroDados').val("");
                        document.getElementById("FiltroDados").setAttribute('placeholder', "Digite o NCM");
                        $('#FiltroDados').unmask();
                    }
                    if (value == "1") {
                        $('#FiltroDados').val("");
                        document.getElementById("FiltroDados").setAttribute('placeholder', "Digite o CEST");


                        $("#FiltroDados").mask("99.999.99");



                    }




                });

            });


        </script>

