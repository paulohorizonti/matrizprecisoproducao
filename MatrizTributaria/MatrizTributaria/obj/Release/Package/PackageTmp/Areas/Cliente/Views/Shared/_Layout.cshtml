                                                            title: 'Relação Cliente x Matriz',
                                                            sliceVisibilityThreshold: .00001,
                                                            pieHole: 0.3,
                                                            pieSliceText: 'value',
                                                            pieSliceTextStyle: {
                                                                color: 'black',
                                                                fontSize: '10'
                                                            },
                                                            legend: 'true'
                                                        };

                                                var chart = new google.visualization.PieChart(document.getElementById('aliIcmsVarConsFinal'));
                                                //selecionando a parte do grafico para chamar a view correspondente
