        <script>
            /*Aliq ICMS Compra de Industria */
            $(document).ready(function () {

                google.charts.load("current", { packages: ["corechart"] });

                google.charts.setOnLoadCallback(aliIcmsComInd); //Edit concluido
                google.charts.setOnLoadCallback(aliIcmsSTComInd); //Edit concluido

                google.charts.setOnLoadCallback(aliIcmsComAta); //Edit concluído
                google.charts.setOnLoadCallback(aliIcmsSTComAta); //Edit concluído

                google.charts.setOnLoadCallback(aliIcmsComSN); //Edit concluido
                google.charts.setOnLoadCallback(aliIcmsSTComSN); //Edit concluido

                google.charts.setOnLoadCallback(aliIcmsNFEInd); //edit concluido
                google.charts.setOnLoadCallback(aliIcmsNFEAta); //edit concluido
                google.charts.setOnLoadCallback(aliIcmsNFESN);  //edit concluido


                function aliIcmsComInd() {
                        var data = google.visualization.arrayToDataTable([
                            ['Aliquota', 'Valores'],
                            ['Com aliquota', @Html.Raw(ViewBag.AliqICMSEntradaInd)],
                            ['Sem aliquota', @Html.Raw(ViewBag.AliqICMSEntradaNulla)]

                        ]);

                        var options = {
                            title: 'Dados da Matriz Tributária',
                            sliceVisibilityThreshold: .00001,
                            pieHole: 0.3,
                            pieSliceTextStyle: {
                            color: 'black',
                                fontSize: '10'
                            },
                            legend: 'true'
                        };

                    var chart = new google.visualization.PieChart(document.getElementById('aliIcmsComInd'));
                    //selecionando a parte do grafico para chamar a view correspondente
                    function selectHandler() {
                        var selectedItem = chart.getSelection()[0]; //pegando qual opção foi selecionada no grafico
                        //se vier alguma coisa
                        if (selectedItem) {
                            var topping = data.getValue(selectedItem.row, 0); //pega o valor do item selecionado e atribui à variavel topping

                            //agora analisa essa variavel para ver qual o dado está contida
                            if (topping) {
                                bloqueioTela();

                                /*Função para chamar a action com os dados solicitados via ajax*/
                                $.ajax({

                                    data: { id: topping },
                                    types: "GET",
                                    processData: true,
                                    success: function () {
                                        window.location.href = '/Tributacao/EditAliqIcmsCompIndMassa?opcao=' + topping;
                                    }

                                }); //fim ajax
                            }//fim if topping

                        }//fim if selecteditem

                    }//fim da função handler

                    //chmando a view
                    google.visualization.events.addListener(chart, 'select', selectHandler);//chamando a funcao handler
                    chart.draw(data, options);
                }
